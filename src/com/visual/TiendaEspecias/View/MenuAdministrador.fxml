            <ImageView fx:id="imagenLogo" fitHeight="120.0" fitWidth="200.0" layoutX="118.0" layoutY="136.0" pickOnBounds="true" preserveRatio="true" />
            <Label layoutX="117.0" layoutY="286.0" text="ESPECIAS &amp; CONDIMENTOS">
               <font>
                  <Font name="AveriaSerif-Regular" size="15.0" />
