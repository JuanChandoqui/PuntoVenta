<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.visual.TiendaEspecias.Controller.MenuVendedorController">
   <center>
      <AnchorPane prefHeight="572.0" prefWidth="435.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="imagenLogo" fitHeight="120.0" fitWidth="200.0" layoutX="118.0" layoutY="136.0" pickOnBounds="true" preserveRatio="true" />
            <Label layoutX="117.0" layoutY="286.0" text="ESPECIAS &amp; CONDIMENTOS">
               <font>
                  <Font name="AveriaSerif-Regular" size="15.0" />
               </font>
            </Label>
            <Label fx:id="fecha" layoutX="117.0" layoutY="349.0">
               <font>
                  <Font name="AveriaSerif-Regular" size="15.0" />
               </font>
            </Label>
         </children></AnchorPane>
   </center>
   <left>
      <AnchorPane prefHeight="560.0" prefWidth="241.0" style="-fx-background-color: #008866;" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="97.0" layoutY="87.0" text="MENÚ" textFill="#fffafa">
               <font>
                  <Font name="AveriaSerif-Regular" size="16.0" />
               </font>
            </Label>
            <Button fx:id="botonListaProductos" layoutX="71.0" layoutY="220.0" mnemonicParsing="false" onMouseClicked="#botonListaProductosOnMouseClicked" text="Lista Productos" />
