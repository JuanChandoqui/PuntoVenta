            <Button fx:id="botonSalir" layoutX="88.0" layoutY="354.0" mnemonicParsing="false" onMouseClicked="#botonSalirOnMouseClicked" prefHeight="25.0" prefWidth="66.0" text="Salir" />
         </children></AnchorPane>
   </left>
   <top>
      <AnchorPane prefHeight="48.0" prefWidth="400.0" style="-fx-background-color: #263238;" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
      <AnchorPane prefHeight="48.0" prefWidth="400.0" style="-fx-background-color: #263238;" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
